<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/index.css" rel="stylesheet" type="text/css">
    <title></title>
</head>
<body>
    <div class="head">
        <h1 class="h1">HODLINFO</h1>
        <div class="subhead">
            <select>
                <option>INR</option>
            </select>
    
            <select>
                <option>BTC</option>
                <option>ETH</option>
                <option>USDT</option>
                <option>XRP</option>
                <option>TRX</option>
                <option>DASH</option>
            </select>
    
            <button>BUY BTC</button>
        </div>
    </div>


    <div class="curr">
        <span>
            <h4>0.24%</h4>
            <p>5 mins</p>
        </span>

        <span>
            <h4>0.24%</h4>
            <p>1 Hour</p>
        </span>

        <span>
            <p>Best Price to Trade</p>
            <h2>$ 26,56,110</h2>
            <p class="avg">Average BTC/INR net price including commission</p>
        </span>

        <span>
            <h4>9.99%</h4>
            <p>1 Day</p>
        </span>

        <span>
            <h4>16.73%</h4>
            <p>7 Days</p>
        </span>
    </div>

    <div>
        <table>
            <tr>
              <th>#</th>
              <th>Platform</th>
              <th>Last Traded Price</th>
              <th>Buy/Sell Price</th>
              <th>Difference</th>
              <th>Savings</th>
            </tr>

            <% data.forEach((ele) => { %>
                <tr>
                    <td><%= ele.name %></td>
                    <td>Wazix</td>
                    <td>$25,74,887</td>
                    <td>$25,74,887/$25,74,887</td>
                    <td>-3.14%</td>
                    <td>$83,498</td>
                </tr>
            <% }) %>
           
          </table>
    </div>
</body>
</html>